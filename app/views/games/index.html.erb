<h2 class="center">Games</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
    <button class="btn" id="toggle">Show Form</button>
    <ul>
      <% @games.each do |game| %>
      <%= render parital: 'game', locals: {game: game} %>
      <% end %> 
    </ul>
  </div>
  <div class="row" id="character-list">
    <h3 id="game">Characters in Game</h2>
    <ul id="characters">
    </ul>
  </div>
</div>
